{
  "name": "idea",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --no-clean",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "eleventy --serve",
    "build:content": "eleventy",
    "dev": "gulp dev",
    "tokens": "style-dictionary build --config ./tokens.config.js",
    "js-tokens": "style-dictionary build --config ./tokens.config.json",
    "np:major": "np major --yolo=true --no-publish",
    "np:minor": "np minor --yolo=true --no-publish",
    "np:patch": "np patch --yolo=true --no-publish"
  },
  "devDependencies": {},
  "main": "index.js",
  "repository": "https://github.com/shawn-sandy/idea.git",
  "author": "shawnsandy <shawnsandy04@gmail.com>",
