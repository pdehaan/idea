{
  "name": "idea",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "lerna run --scope @local/idea start",
    "np:major": "np major --yolo=true --no-publish",
    "np:minor": "np minor --yolo=true --no-publish",
    "np:patch": "np patch --yolo=true --no-publish",
    "changelog": "auto-changelog -p --template config/_changelog.hbs"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.2",
    "snowpack": "^1.4.0"
  },
  "auto-changelog": {
    "unreleased": true,
    "template": "config/_changelog.hbs"
  },
  "main": "index.js",
  "repository": "https://github.com/shawn-sandy/idea.git",
  "author": "shawnsandy <shawnsandy04@gmail.com>",
  "license": "MIT"
}
